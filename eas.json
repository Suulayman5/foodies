{
  "cli": {
    "version": ">= 3.15.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "cache": {
        "disabled": false
      }
    },
    "preview": {
      "developmentClient": false,
      "distribution": "internal",
      "cache": {
        "disabled": false
      }
    },
    "production": {
      "autoIncrement": true,
      "developmentClient": false,
      "distribution": "store",
      "cache": {
        "disabled": false
      }
    }
  },
  "submit": {
    "production": {}
  }
}
